curl -X POST http://localhost:3000/api/webhooks \
  -H "Content-Type: application/json" \
  -d '{"url": "https://example.com/webhook"}'


curl -X POST http://localhost:3000/api/events \
  -H "Content-Type: application/json" \
  -d '{
    "bookingId": "BK123456",
    "status": "dibayar",
    "timestamp": "2023-05-15T10:30:00Z"
  }'